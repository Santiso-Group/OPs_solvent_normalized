tolerance 2.0 # tolerance distance
output simbox.pdb # output file name
filetype pdb # output file type
seed -1 # randomization
#
# Create a box of urea and acn molecules
#
# urea
structure urea.pdb
number 10 # Number of molecules
inside cube 0. 0. 0. 60. # x, y, z coordinates of box, and length of box in Angstroms
end structure
# acetonitrile
structure acn.pdb
number 2110
inside cube 0. 0. 0. 60.
end structure